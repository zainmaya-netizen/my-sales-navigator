{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run --logHeapUsage",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "postinstall": "git config core.hooksPath .githooks && chmod +x .githooks/pre-commit",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n src/config/typeorm.config.seed.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/typeorm.config.migration.ts",
    "migration:generate": "npm run typeorm -- -d ./src/config/typeorm.config.migration.ts migration:generate ./$npm_config_path/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./$npm_config_path/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
    "migration:drop": "npm run typeorm -- -d src/config/typeorm.config.migration.ts schema:drop",
    "setup": "pnpm i && npm run migration:drop && npm run migration:run && npm run seed:run && npm run test:cov",
    "migration:refresh": "npm run migration:drop && npm run migration:run"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.3.8",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.8",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.8",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@swc/core": "^1.3.35",
    "@swc/helpers": "^0.4.14",
    "@types/nodemailer": "^6.4.7",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nestjs-seeder": "^0.3.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.12",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.8",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.13.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-node": "^3.0.2",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5",
    "webpack": "^5.75.0"
  }
}

